{
    "atlascode.jira.todoIssues.triggers": ["!!", "BUG", "TODO", "- [ ]"],
    "cssToStylus.indent": 4,
    "editor.codeActionsOnSave": {},
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "emmet.includeLanguages": {
        "postcss": "css"
    },
    "env-sync.envDestination": [".env.example"],
    "env-sync.envSource": ".env",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*": "${capture}.example,${capture}.sample",
        "*.config.js": ".${capture}ignore",
        "*.config.json": ".${capture}ignore",
        "*.config.jsonc": ".${capture}ignore",
        "*.config.ts": ".${capture}ignore",
        "*.json": "${capture}ignore",
        "*.mustache": "${capture}",
        "*.pug": "${capture}.html",
        "*.styl": "${basename}.css",
        "*.ts": "${capture}.js,${capture}.gs",
        "*rc": ".${capture}ignore",
        "*rc.json": "${capture}ignore",
        "*rc.jsonc": "${capture}ignore",
        ".*attributes": ".${capture}ignore,.${capture}modules",
        "*.editorconfig": "${capture}.editorconfig-checker.*",
        "*.gitattributes": "${capture}.git-blame-ignore-revs",
        ".tool-versions": "mise.toml,mise.lock",
        "package.json": "bun.lock*,yarn.lock,package-*.json,bunfig.toml,*.npmrc,tsconfig.*",
        "pyproject.toml": "*.pdm-python,pdm.lock",
        "bunfig.toml": "build.bun.js,build.bun.ts"
    },
    "files.associations": {
        "**.env*": "dotenv",
        "**.gs": "javascript",
        "**.stylintrc": "jsonc",
        "**.user.css": "postcss",
        "**/.github/workflows/*.yml": "github-actions-workflow",
        "**/LICENSE": "plaintext",
        ".*ignore": "ignore",
        "**/.git/config": "gitconfig"
    },
    "files.encoding": "utf8",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.readonlyInclude": {
        "**.user.css": true,
        "./.history/**": true,
        "./build/**": true,
        "./src/**.class": true,
        "./src/**.css": true,
        "./src/**.{js,mjs,cjs}": true
    },
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.trunk/*actions/": true,
        "**/.trunk/*logs/": true,
        "**/.trunk/*notifications/": true,
        "**/.trunk/*out/": true,
        "**/.trunk/*plugins/": true,
        "./.history/**": true
    },
    "gitconfig-lsp.format.gitattributes.hardTab": false,
    "gitconfig-lsp.format.gitattributes.hardTabWidth": 4,
    "gitconfig-lsp.format.gitattributes.tabSize": 16,
    "java.format.settings.url": "./.trunk/configs/java-formatter.xml",
    "java.help.showReleaseNotes": false,
    "java.project.outputPath": "src",
    "java.project.referencedLibraries": [],
    "java.project.sourcePaths": ["src"],
    "material-icon-theme.files.associations": {
        "**.clasp.json": "Apps-script",
        "**.claspignore": "Apps-script",
        "**.user.css": "postcss",
        ".vscode/settings.json": "settings",
        "appsscript.json": "Apps-script",
        ".git/config": "git",
        "*.bun.ts": "bun-ts",
        "*.bun.js": "bun-js"
    },
    "material-icon-theme.files.customClones": [
        {
            "name": "bun-ts",
            "base": "bun",
            "color": "#3903fc"
        },
        {
            "name": "bun-js",
            "base": "bun",
            "color": "#62fc03"
        }
    ],
    "material-icon-theme.folders.associations": {
        ".vscode/instructions": "rules",
        ".github/instructions": "rules"
    },
    "peacock.color": "E2830E",
    "prettier.useEditorConfig": true,
    "shellformat.useEditorConfig": true,
    "stylelint.customSyntax": "stylelint-stylus/custom-syntax",
    "stylelint.validate": ["stylus"],
    "stylusSupremacy.alwaysUseAtBlock": false,
    "stylusSupremacy.alwaysUseExtends": false,
    "stylusSupremacy.alwaysUseImport": false,
    "stylusSupremacy.alwaysUseNoneOverZero": true,
    "stylusSupremacy.alwaysUseNot": false,
    "stylusSupremacy.alwaysUseZeroWithoutUnit": true,
    "stylusSupremacy.insertBraces": false,
    "stylusSupremacy.insertColons": false,
    "stylusSupremacy.insertLeadingZeroBeforeFraction": false,
    "stylusSupremacy.insertNewLineAroundBlocks": true,
    "stylusSupremacy.insertNewLineAroundImports": true,
    "stylusSupremacy.insertNewLineAroundOthers": true,
    "stylusSupremacy.insertNewLineAroundProperties": true,
    "stylusSupremacy.insertNewLineBeforeElse": false,
    "stylusSupremacy.insertParenthesisAfterNegation": false,
    "stylusSupremacy.insertParenthesisAroundIfCondition": false,
    "stylusSupremacy.insertSemicolons": false,
    "stylusSupremacy.insertSpaceAfterComma": true,
    "stylusSupremacy.insertSpaceAfterComment": true,
    "stylusSupremacy.insertSpaceBeforeComment": true,
    "stylusSupremacy.insertSpaceInsideParenthesis": false,
    "stylusSupremacy.newLineChar": "\n",
    "stylusSupremacy.preserveNewLinesBetweenPropertyValues": false,
    "stylusSupremacy.quoteChar": "\"",
    "stylusSupremacy.reduceMarginAndPaddingValues": false,
    "stylusSupremacy.selectorSeparator": "\n",
    "stylusSupremacy.sortProperties": "alphabetical",
    "stylusSupremacy.tabStopChar": "    ",
    "task.allowAutomaticTasks": "on",
    "todo-tree.filtering.excludeGlobs": ["**/node_modules/**", "./build/**", "**.user.css", "./src/**.css", "./src/**.{js,mjs,cjs}", "./src/**.class", "./build/**", "./.history/**", "./target/**"],
    "todo-tree.filtering.includeHiddenFiles": true,
    "todo-tree.general.exportPath": "./todos.md",
    "todo-tree.general.tags": ["!!", "BUG", "TODO", "- [ ]"],
    "todo-tree.highlights.backgroundColourScheme": [],
    "todo-tree.highlights.customHighlight": {
        "!!": {
            "background": "#ff0000",
            "icon": "alert",
            "iconColour": "#ff0000"
        },
        "BUG": {
            "background": "#ff0000",
            "icon": "alert",
            "iconColour": "#ff0000"
        },
        "TODO": {
            "background": "#00ff00",
            "icon": "check",
            "iconColour": "#00ff00"
        },
        "- [ ]": {
            "background": "#00ff00",
            "icon": "check",
            "iconColour": "#00ff00"
        }
    },
    "todo-tree.highlights.enabled": true,
    "todo-tree.highlights.foregroundColourScheme": [],
    "todo-tree.regex.enableMultiLine": true,
    "todo-tree.ripgrep.usePatternFile": true,
    "todo-tree.tree.buttons.export": true,
    "todo-tree.tree.hideTreeWhenEmpty": true,
    "todo-tree.tree.labelFormat": "${tag} ${after}",
    "todo-tree.tree.scanMode": "workspace",
    "todo-tree.tree.showBadges": true,
    "todo-tree.tree.showCountsInTree": true,
    "todo-tree.tree.sortTagsOnlyViewAlphabetically": false,
    "todo-tree.tree.tooltipFormat": "${filepath}, line ${line}",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#f29c31",
        "activityBar.background": "#f29c31",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#e1fdf1",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#f29c31",
        "statusBar.background": "#e2830e",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#b2670b",
        "statusBarItem.remoteBackground": "#e2830e",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#e2830e",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#e2830e99",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "yaml.schemas": {
        "https://docs.renovatebot.com/renovate-schema.json": "**/renovate.json*,**.renovaterc",
        "https://json.schemastore.org/pre-commit-config.json": "**/.github/hooks/.pre-commit-config.{yml,yaml}",
        "https://raw.githubusercontent.com/megalinter/megalinter/main/megalinter/descriptors/schemas/megalinter-configuration.jsonschema.json": "**/.mega-linter.yml",
        "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json": "**/*cspell.json*",
        "https://www.schemastore.org/appsscript.json": "**/appsscript.json",
        "https://www.schemastore.org/bunfig.json": "**/bunfig.toml",
        "https://www.schemastore.org/clasp.json": "**.clasp.json",
        "https://www.schemastore.org/github-workflow.json": "**/.github/workflows/**.{yml,yaml}"
    },
    "path-intellisense.mappings": {
        "/": "${workspaceFolder}/"
    },
    "path-intellisense.pathSeparator": "/",
    "path-intellisense.autoSlashAfterDirectory": true,
    "cursor.cpp.disabledLanguages": [],
    "cursor.general.globalCursorIgnoreList": [
        "**/.env",
        "**/credentials.json",
        "**/credentials.*.json",
        "**/secret.json",
        "**/secrets.json",
        "**/*.key",
        "**/*.pem",
        "**/*.pfx",
        "**/*.p12",
        "**/*.crt",
        "**/*.cer",
        "**/id_rsa",
        "**/id_dsa",
        "**/.ssh/id_*"
    ],
    "json.schemaDownload.enable": true,
    "github.copilot.enable": {
        "*": true,
        "scminput": true,
        "markdown": true,
        "plaintext": true
    },
    "chat.instructionsFilesLocations": {
        ".cursor/rules": true
    },
    "chat.promptFilesLocations": {
        ".cursor/prompts": true
    },
    "trunk.addToolsToPath": false,
    "trunk.autoInit": false,
    "trunk.enableUpgradePane": false,
    "trunk.inlineDecoratorsForAllExtensions": true,
    "trunk.trace.server": "messages",
    "editor.fontFamily": "fira-code-nerd, 'FiraCode Nerd Font', FiraCode, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "all",
    "editor.experimentalWhitespaceRendering": "font",
    "editor.codeLensFontFamily": "fira-code-nerd, 'FiraCode Nerd Font', FiraCode, Consolas, 'Courier New', monospace",
    "debug.console.fontFamily": "fira-code-nerd, 'FiraCode Nerd Font', FiraCode, Consolas, 'Courier New', monospace",
    "editor.inlineSuggest.fontFamily": "fira-code-nerd, 'FiraCode Nerd Font', FiraCode, Consolas, 'Courier New', monospace",
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.updateBuildConfiguration": "automatic",
    "bruno.defaultCollectionLocation": "./bruno"
}
